<Page
    x:Class="waerp_toolpilot.main.mainDashboard"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:waerp_toolpilot.main"
    xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="mainDashboard"
    d:DesignHeight="600"
    d:DesignWidth="1086.625"
    Background="#9E9E9E"
    FontFamily="{DynamicResource MaterialDesignFont}"
    Foreground="black"
    TextElement.FontSize="13"
    TextElement.FontWeight="Regular"
    mc:Ignorable="d">
    <Page.Resources>
        <Style x:Key="QuantityCell" TargetType="{x:Type TextBlock}">
            <Setter Property="TextAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
        <Style x:Key="SingleColDataGrid" TargetType="{x:Type TextBlock}">
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="Margin" Value="10,0,0,0" />
        </Style>
        <Style x:Key="GridCellPad" TargetType="{x:Type TextBlock}">
            <Setter Property="VerticalAlignment" Value="Center" />

            <Setter Property="Margin" Value="10,0,0,0" />
        </Style>
        <Style x:Key="LeftGridHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Left" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Foreground" Value="#444444" />
            <Setter Property="Margin" Value="15,0,0,0" />
            <Setter Property="FontWeight" Value="Medium" />
            <Setter Property="FontSize" Value="12" />
            <Setter Property="Cursor" Value="Hand" />
        </Style>
        <Style x:Key="CenterGridHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center" />
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Foreground" Value="#444444" />
            <Setter Property="FontWeight" Value="Medium" />
            <Setter Property="FontSize" Value="12" />
            <Setter Property="Cursor" Value="Hand" />
        </Style>


    </Page.Resources>
    <materialDesign:Card Background="#9E9E9E">
        <!--<TabControl
            HorizontalAlignment="Stretch"
            materialDesign:ColorZoneAssist.Mode="PrimaryLight"
            Style="{StaticResource MaterialDesignFilledTabControl}">-->
        <!--<TabItem Header="{DynamicResource dashboardTabItem1}">-->
        <Grid Style="{StaticResource GridBackgroundMainFrame}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="0.5*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <StackPanel
                Grid.Row="0"
                Grid.Column="0"
                Margin="20,20,0,0"
                Orientation="Vertical">
                <TextBlock
                    Grid.Row="0"
                    FontSize="20"
                    Text="{DynamicResource dashboardTitleWelcome}" />
                <TextBlock
                    x:Name="fullname"
                    Grid.Row="1"
                    FontSize="20"
                    FontWeight="Light"
                    Text="{Binding Fullname}" />
            </StackPanel>


            <!--<lvc:PieChart
                        x:Name="StockOverviewChart"
                        Grid.Row="1"
                        DataTooltip="{x:Null}"
                        Hoverable="False"
                        LegendLocation="Bottom">
                        <lvc:PieChart.Series>
                            <lvc:PieSeries
                                Title="Entnommen"
                                DataLabels="True"
                                LabelPoint="{Binding PointLabel}"
                                Values="3" />
                            <lvc:PieSeries
                                Title="Eingelagert"
                                DataLabels="True"
                                Foreground="Black"
                                LabelPoint="{Binding PointLabel}"
                                Values="4" />
                            <lvc:PieSeries
                                Title="Verschrottet"
                                DataLabels="True"
                                Foreground="Black"
                                LabelPoint="{Binding PointLabel}"
                                Values="4" />
                        </lvc:PieChart.Series>
                    </lvc:PieChart>-->

            <StackPanel
                Grid.RowSpan="2"
                Grid.Column="1"
                Margin="0,20,10,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Orientation="Horizontal">
                <Button
                    x:Name="changeView"
                    Grid.Row="0"
                    Grid.Column="1"
                    Margin="0,0,20,0"
                    HorizontalAlignment="Right"
                    Click="changeView_Click"
                    IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                    Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}">
                    <materialDesign:PackIcon
                        Width="20"
                        Height="20"
                        Kind="ViewDashboard" />
                </Button>
                <Border
                    Width="190"
                    Height="70"
                    Margin="0,0,0,0"
                    Background="#FFFFFF"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10"
                    Visibility="Visible">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Row="0" Margin="0,0,10,0">
                            <TextBlock
                                x:Name="currentRents"
                                HorizontalAlignment="Right"
                                FontSize="30"
                                Foreground="#34914E"
                                Text="0" />
                            <TextBlock HorizontalAlignment="Right" Text="Aktuell entnommene Artikel" />
                        </StackPanel>
                    </Grid>
                </Border>
                <Border
                    Width="190"
                    Height="70"
                    Margin="10,0,0,0"
                    Background="#FFFFFF"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10"
                    Visibility="Visible">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Row="0" Margin="0,0,10,0">
                            <TextBlock
                                x:Name="currentOrders"
                                HorizontalAlignment="Right"
                                FontSize="30"
                                Foreground="#34914E"
                                Text="0" />
                            <TextBlock HorizontalAlignment="Right" Text="Aktuell bestellte Artikel" />
                        </StackPanel>
                    </Grid>
                </Border>
                <Border
                    Width="190"
                    Height="70"
                    Margin="0,0,0,0"
                    Background="#F4F4F4"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10"
                    Visibility="Collapsed">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Row="0" Margin="0,0,0,0">
                            <TextBlock
                                x:Name="nextCheckUpDate"
                                HorizontalAlignment="Right"
                                FontSize="30"
                                Foreground="#34914E"
                                Text="0" />
                            <TextBlock HorizontalAlignment="Right" Text="{DynamicResource dashboardAlert1}" />
                        </StackPanel>
                    </Grid>
                </Border>
                <Border
                    Width="190"
                    Height="70"
                    Background="#F4F4F4"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10"
                    Visibility="Collapsed">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Row="0" Margin="0,0,10,0">
                            <TextBlock
                                HorizontalAlignment="Right"
                                FontSize="30"
                                Foreground="#34914E"
                                Text="0" />
                            <TextBlock HorizontalAlignment="Right" Text="{DynamicResource dashboardAlert2}" />
                        </StackPanel>
                    </Grid>
                </Border>
            </StackPanel>

            <Button
                Grid.Row="0"
                Grid.Column="1"
                Width="50"
                Height="50"
                Margin="0,0,30,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Bottom"
                Click="OpenBarcodeScan_Click"
                Style="{StaticResource RoundButtons}"
                ToolTip="Artikelsuche/scannen"
                Visibility="Hidden">
                <materialDesign:PackIcon
                    Width="25"
                    Height="25"
                    Kind="DatabaseSearch" />
            </Button>
            <Border
                Grid.Row="1"
                Grid.Column="0"
                Margin="10,10,10,10"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Background="#FFFFFF"
                BorderBrush="#B8B8B8"
                BorderThickness="2"
                CornerRadius="10">
                <Grid Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.5*" />
                        <RowDefinition Height="4*" />

                    </Grid.RowDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        FontSize="16"
                        FontWeight="SemiBold"
                        Foreground="#333333"
                        TextWrapping="Wrap">
                        Aktivitäten (aktuelle Woche)
                    </TextBlock>
                    <lvc:CartesianChart
                        x:Name="mainGraph"
                        Grid.Row="1"
                        LegendLocation="Right"
                        Series="{Binding OverviewSeries}">
                        <lvc:CartesianChart.AxisY>
                            <lvc:Axis Title="Artikel" LabelFormatter="{Binding YFormatter}" />
                        </lvc:CartesianChart.AxisY>
                        <lvc:CartesianChart.AxisX>
                            <lvc:Axis Title="Tag" Labels="{Binding XLabels}" />
                        </lvc:CartesianChart.AxisX>
                    </lvc:CartesianChart>
                </Grid>
            </Border>
            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />

                </Grid.ColumnDefinitions>
                <Border
                    Grid.Column="0"
                    Margin="10,10,10,10"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Background="#FFFFFF"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="4*" />

                        </Grid.RowDefinitions>
                        <TextBlock
                            Grid.Row="0"
                            FontSize="16"
                            FontWeight="SemiBold"
                            Foreground="#333333"
                            TextWrapping="Wrap">
                            Artikelstatistik (aktuelles Quartal)
                        </TextBlock>
                        <lvc:PieChart
                            x:Name="Statistic1"
                            Grid.Row="1"
                            DataTooltip="{x:Null}"
                            Hoverable="False"
                            LegendLocation="Bottom" />
                    </Grid>
                </Border>
                <Border
                    Grid.Column="1"
                    Margin="10,10,10,10"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Background="#FFFFFF"
                    BorderBrush="#B8B8B8"
                    BorderThickness="2"
                    CornerRadius="10">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="4*" />

                        </Grid.RowDefinitions>
                        <TextBlock
                            Grid.Row="0"
                            FontSize="16"
                            FontWeight="SemiBold"
                            Foreground="#333333"
                            TextWrapping="Wrap">
                            Lieferantenstatistik (Insgesamt)
                        </TextBlock>
                        <lvc:PieChart
                            x:Name="Statistic2"
                            Grid.Row="1"
                            DataTooltip="{x:Null}"
                            Hoverable="False"
                            LegendLocation="Bottom" />
                    </Grid>
                </Border>
            </Grid>

            <Border
                Grid.Row="2"
                Grid.Column="0"
                Margin="10,10,10,10"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Background="#FFFFFF"
                BorderBrush="#B8B8B8"
                BorderThickness="2"
                CornerRadius="10">
                <Grid Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="4*" />

                    </Grid.RowDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        FontSize="16"
                        FontWeight="SemiBold"
                        Foreground="#333333"
                        TextWrapping="Wrap">
                        Getätigte Bestellungen (aktuelle Woche)
                    </TextBlock>
                    <lvc:CartesianChart
                        x:Name="vendorOrderGraph"
                        Grid.Row="1"
                        LegendLocation="Right"
                        Series="{Binding OverviewSeries}">
                        <lvc:CartesianChart.AxisY>
                            <lvc:Axis Title="Bestellte Artikel" LabelFormatter="{Binding YFormatter}" />
                        </lvc:CartesianChart.AxisY>
                        <lvc:CartesianChart.AxisX>
                            <lvc:Axis Title="Tag" Labels="{Binding XLabels}" />
                        </lvc:CartesianChart.AxisX>
                    </lvc:CartesianChart>
                </Grid>
            </Border>
            <Border
                Grid.Row="2"
                Grid.Column="1"
                Margin="10,10,10,10"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                Background="#FFFFFF"
                BorderBrush="#B8B8B8"
                BorderThickness="2"
                CornerRadius="10">
                <Grid Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="4*" />

                    </Grid.RowDefinitions>
                    <TextBlock
                        FontSize="16"
                        FontWeight="SemiBold"
                        Foreground="#333333"
                        TextWrapping="Wrap">
                        Maschinenübersicht
                    </TextBlock>
                    <DataGrid
                        x:Name="MachineDataItems"
                        Grid.Row="1"
                        Background="#FFFFFF"
                        ItemsSource="{Binding Path=machineData}">
                        <DataGrid.CellStyle>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                                <Setter Property="FontSize" Value="14" />
                                <Setter Property="MinHeight" Value="30" />
                                <Setter Property="MaxHeight" Value="30" />
                                <Style.Triggers>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter Property="Background" Value="#F4F4F4" />
                                        <Setter Property="Foreground" Value="Black" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </DataGrid.CellStyle>
                        <DataGrid.Columns>
                            <DataGridTextColumn
                                Width="*"
                                Binding="{Binding Path=name}"
                                ElementStyle="{StaticResource GridCellPad}"
                                Header="{DynamicResource machineNameHeader}"
                                HeaderStyle="{StaticResource LeftGridHeaderStyle}"
                                IsReadOnly="True" />
                            <DataGridTextColumn
                                Width="*"
                                Binding="{Binding Path=machine_group_index}"
                                ElementStyle="{StaticResource QuantityCell}"
                                Header="Gruppenindex"
                                HeaderStyle="{StaticResource CenterGridHeaderStyle}"
                                IsReadOnly="True" />
                            <DataGridTextColumn
                                Width="*"
                                Binding="{Binding Path=quantity}"
                                ElementStyle="{StaticResource QuantityCell}"
                                Header="{DynamicResource activeToolsHeader}"
                                HeaderStyle="{StaticResource CenterGridHeaderStyle}"
                                IsReadOnly="True" />
                            <DataGridTextColumn
                                Width="*"
                                Binding="{Binding Path=last_employee}"
                                ElementStyle="{StaticResource QuantityCell}"
                                Header="letzter Mitarbeiter"
                                HeaderStyle="{StaticResource LeftGridHeaderStyle}"
                                IsReadOnly="True" />


                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </Border>

